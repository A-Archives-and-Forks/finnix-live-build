[Unit]
Description=Shared SSH agent init (%i)
Before=getty.target
After=nss-user-lookup.target

[Service]
ExecStart=/usr/lib/{{ PRODUCT_ID }}/shared-ssh-agent
User=%i

[Install]
WantedBy=multi-user.target
